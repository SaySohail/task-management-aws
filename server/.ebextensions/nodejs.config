files:
  "/opt/elasticbeanstalk/hooks/appdeploy/pre/01_install_node.sh":
    mode: "000755"
    owner: root
    group: root
    content: |
      #!/bin/bash
      export NVM_DIR="$HOME/.nvm"
      [ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"
      nvm install 20
      nvm use 20
      nvm alias default 20